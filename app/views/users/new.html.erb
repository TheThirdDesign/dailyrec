
<div id="header">
  <div id="title">Contact Us</div>
</div>
<div class="row">

  <div class="col-md-6">
  </div>

  <div class="col-md-6">
    <div id="body">
    <div id="container1">
      <div id="form">
        <%= form_for @user do |f| %>
             <%= f.label :name %>:
             </br>
             <%= f.text_field :name %>
           </br>
             <%= f.label :email %>:
             </br>
             <%= f.email_field :email %>
             </br>
             <%= f.label :message, id: "messageLabel1" %>:
             </br>
             <%= f.text_area :message, id: "messageArea" %>
              </br>
              <%= f.submit "Send" %>
            <% end %>
      </div>
    </div>
  </div>
</div>
</div>
