a.navbar-brand {
margin-top: -50px;
margin-bottom: 30px;
}

div#navbar-collapse-1 {
border: none;
}

ul.nav.navbar-nav.navbar-left {
width: 100%;
}

li.link {
width: 20%;
text-align: center;
}

.navbar-default div div#logo {
display: none;
}

.navbar-left {
height: 100px;
}

#logo img {
width: 192px;
}

.navbar-default {
background-color: #f8f8f8;
border: none;
}

body {
  display: flex;
}

div#window {
  width: 99%;
  margin: 7px;
  border: 2px solid #00008F;
  border-right: 2px solid #00008F;
}

div#menu {
  margin-left: 5%;
  width: 90%;
  height: 125;
  display: inline-flex;
  font-family: GothamNarrow;
  text-decoration: none;
}

.menuLink {
  width: 20%;
  height: 125;
  display: inline-flex;
  text-decoration: none;
}

.menuLinkContents {
  margin: auto;
  display: inline-block;
}

#logo {
  position: relative;
  top: 0;

}

#logo img {
  width: 250;
  height: 125;
  }
.menulink a {
  text-decoration: none;
}

.menuLink a p {
  margin: auto;
  width:100%;
  text-decoration: none;
  color: rgb(0,0,120);
  font-size: 20px;
  letter-spacing: 3;
  text-align: center;
}

.menuLink h6 {
  margin: auto;
  width: 100%;
  font-size: 11px;
  text-align: center;
  padding-top: 5;
}

div#middle {
  width: 100%;
  height: 600px;
  display: flex;
}

div#container1 {
  width: 90%;
  height: 520px;
  background-image: url("overlay.png"),url("bham.jpg");
  background-size: cover;
  margin: auto;
  display: flex;

}

div#intro {
  width: 100%;
  height: 500px;
  margin: 7px;
  border: 2px solid white;
  text-align: center;
  font-family: GothamNarrow;
  color: white;
}

div#intro h1 {
  position: relative;
  font-size: 50px;
  top: 75px;
}

div#intro p {
position: relative;
width: 70%;
top: 90px;
font-size: 25px;
word-wrap: inherit;
margin: auto;
letter-spacing: 2px;
}

div#intro p italic {
  font-family: GothamNarrowItalic;
}

div#footer {
  width: 100%;
  height: 100px;
  display: flex;
}

div#copyright {
  width: 100%;
  margin: auto;
  font-family: GothamNarrow;
  padding-top: 40px;
  opacity: .5;
}

div#copyright p {
  text-align: center;
}

div#copyright p a {
  text-decoration: none;
  color: #00008F;
}
